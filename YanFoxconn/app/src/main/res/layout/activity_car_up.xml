<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--標題-->
    <include layout="@layout/common_title"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginBottom="@dimen/dimen_10">
        <TextView
            android:id="@+id/tv_file_one"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_30"
            android:text="開箱狀態"
            android:gravity="center_vertical"
            android:textSize="@dimen/text_size_16"
            android:layout_marginLeft="@dimen/dimen_10"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <GridView
                android:id="@+id/gv_photo1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_10"
                android:horizontalSpacing="@dimen/dimen_2"
                android:numColumns="4"
                android:scrollbars="none"
                android:verticalSpacing="@dimen/dimen_2" />

            <!--空白圖片佔位-->
            <ImageView
                android:id="@+id/iv_empty1"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="@dimen/dimen_10"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_addpic_unfocused" />

        </RelativeLayout>

        <TextView
            android:id="@+id/tv_file_two"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_30"
            android:gravity="center_vertical"
            android:text="關箱狀態"
            android:textSize="@dimen/text_size_16"
            android:layout_marginLeft="@dimen/dimen_10"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <GridView
                android:id="@+id/gv_photo2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_10"
                android:horizontalSpacing="@dimen/dimen_2"
                android:numColumns="4"
                android:scrollbars="none"
                android:verticalSpacing="@dimen/dimen_2" />

            <!--空白圖片佔位-->
            <ImageView
                android:id="@+id/iv_empty2"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="@dimen/dimen_10"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_addpic_unfocused" />

        </RelativeLayout>

        <TextView
            android:id="@+id/tv_file_three"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_30"
            android:text="鉛封狀態"
            android:gravity="center_vertical"
            android:textSize="@dimen/text_size_16"
            android:layout_marginLeft="@dimen/dimen_10"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <GridView
                android:id="@+id/gv_photo3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_10"
                android:horizontalSpacing="@dimen/dimen_2"
                android:numColumns="4"
                android:scrollbars="none"
                android:verticalSpacing="@dimen/dimen_2" />

            <!--空白圖片佔位-->
            <ImageView
                android:id="@+id/iv_empty3"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="@dimen/dimen_10"
                android:scaleType="centerCrop"
                android:src="@mipmap/icon_addpic_unfocused" />

        </RelativeLayout>

    <TextView
        android:id="@+id/tv_file_four"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_30"
        android:text="派車單"
        android:gravity="center_vertical"
        android:textSize="@dimen/text_size_16"
        android:layout_marginLeft="@dimen/dimen_10"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <GridView
            android:id="@+id/gv_photo4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimen_10"
            android:horizontalSpacing="@dimen/dimen_2"
            android:numColumns="4"
            android:scrollbars="none"
            android:verticalSpacing="@dimen/dimen_2" />

        <!--空白圖片佔位-->
        <ImageView
            android:id="@+id/iv_empty4"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="@dimen/dimen_10"
            android:scaleType="centerCrop"
            android:src="@mipmap/icon_addpic_unfocused" />

    </RelativeLayout>

    <TextView
        android:id="@+id/tv_file_five"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_30"
        android:text="設備交接單"
        android:gravity="center_vertical"
        android:textSize="@dimen/text_size_16"
        android:layout_marginLeft="@dimen/dimen_10"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <GridView
            android:id="@+id/gv_photo5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimen_10"
            android:horizontalSpacing="@dimen/dimen_2"
            android:numColumns="4"
            android:scrollbars="none"
            android:verticalSpacing="@dimen/dimen_2" />

        <!--空白圖片佔位-->
        <ImageView
            android:id="@+id/iv_empty5"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="@dimen/dimen_10"
            android:scaleType="centerCrop"
            android:src="@mipmap/icon_addpic_unfocused" />

    </RelativeLayout>
    <TextView
        android:id="@+id/tv_file_six"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_30"
        android:text="三角木"
        android:gravity="center_vertical"
        android:textSize="@dimen/text_size_16"
        android:layout_marginLeft="@dimen/dimen_10"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <GridView
            android:id="@+id/gv_photo6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimen_10"
            android:horizontalSpacing="@dimen/dimen_2"
            android:numColumns="4"
            android:scrollbars="none"
            android:verticalSpacing="@dimen/dimen_2" />

        <!--空白圖片佔位-->
        <ImageView
            android:id="@+id/iv_empty6"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="@dimen/dimen_10"
            android:scaleType="centerCrop"
            android:src="@mipmap/icon_addpic_unfocused" />

    </RelativeLayout>
</LinearLayout></ScrollView>
</LinearLayout>